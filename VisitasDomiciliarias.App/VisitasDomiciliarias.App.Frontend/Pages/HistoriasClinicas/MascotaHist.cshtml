@page
@model VisitasDomiciliarias.App.Frontend.Pages.MascotaHistModel
@{
    int idHistoria = Model.Historia.HistoriaId;    
}
<hr>
<div class="container">
    <div class="row">
        <div class="col-9">
            <h2 class="text-info">Datos de la Mascota</h2>
        </div>
    </div>
</div>

</br>

<table id="tblMascotas" class="table table-striped table-bordered" style="width: 100%;">

    <thead>
        <tr>
            <th scope="col">PropietarioId</th>
            <th scope="col">MascotaId</th>
            <th scope="col">Nombre</th>
            <th scope="col">Raza</th>
            <th scope="col">Edad</th>
            <th scope="col">Tipo</th>
            <th scope="col">Sexo</th>

        </tr>
    </thead>

    <tbody>
        <tr>
             <tr>
            <td>@Model.Mascota.PropietarioId</td>
            <td>@Model.Mascota.MascotaId</td>
            <td>@Model.Mascota.Nombre</td>
            <td>@Model.Mascota.Raza</td>
            <td>@Model.Mascota.Edad</td>
            <td>@Model.Mascota.Tipo</td>
            <td>@Model.Mascota.Genre</td>

            </tr>

        </tr>

    </tbody>
</table>

<hr>
<a class="btn btn-danger" asp-page="./EditHist" asp-route-idHistoria="@idHistoria">
    <i aria-hidden="true">Regresar</i>
</a>
<hr>